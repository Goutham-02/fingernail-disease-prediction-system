  "disease_information": {
    "name": "eczema",
    "category": "Inflammatory skin condition symptom",
    "definition": "Nail changes, such as pitting and ridges, resulting from eczema (dermatitis) that affects the skin around the nails (nail folds) or the nail matrix.",
    "causes": [
      "Inflammation of the proximal nail fold or nail matrix due to atopic dermatitis, pompholyx (dyshidrotic eczema), or irritant/allergic contact dermatitis."
    ],
    "visual_features": [
      "Coarse pitting (small dents) of the nail plate.",
      "Transverse ridges (Beau's lines) from intermittent inflammation.",
      "Trachyonychia (rough, opaque, 'sandpaper' nails).",
      "Onychomadesis (nail shedding) in severe flare-ups.",
      "Onycholysis (nail lifting).",
      "Cuticle inflammation (paronychia).",
      "Shiny, 'buffed' nails from chronic rubbing/itching."
    ],
    "associated_symptoms": [
      "Itchy, red, scaly, or vesicular (blistering) skin on the hands and fingers, especially around the nail folds."
    ],
    "related_conditions": [
      "Atopic dermatitis",
      "Pompholyx (dyshidrotic eczema)",
      "Allergic contact dermatitis",
      "Irritant contact dermatitis"
    ],
    "medical_treatments": [
      "Potent topical corticosteroids applied to the nail folds.",
      "Intralesional corticosteroid injections.",
      "Topical calcineurin inhibitors (e.g., tacrolimus)."
    ],
    "when_to_see_doctor": [
      "If nail changes are persistent, painful, or skin on hands is severely cracked, weeping, or itchy."
    ],
    "prognosis": "Nail changes tend to improve as the underlying hand eczema is controlled, but it can be a chronic and recurring problem.",
    "references": [
      "https://dermnetnz.org/topics/nail-eczema"
    ]
  }
}