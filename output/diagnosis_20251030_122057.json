  "disease_information": {
    "name": "koilonychia",
    "category": "Nail abnormality",
    "definition": "A nail abnormality where the nail plate is thinned, concave (curved inward), and has raised ridges, resembling the shape of a spoon.",
    "causes": [
      "Iron deficiency anemia (most common cause in adults).",
      "Hemochromatosis (iron overload).",
      "Trauma or occupational exposure to petroleum-based solvents.",
      "Can be normal in infants, but usually self-corrects.",
      "Systemic lupus erythematosus (SLE).",
      "Raynaud's disease."
    ],
    "visual_features": [
      "Thin, fragile nail plate that is concave or 'scooped out' in the center, like a spoon.",
      "The nail may be able to hold a drop of water."
    ],
    "associated_symptoms": [
      "If due to anemia: fatigue, weakness, pale skin, shortness of breath, headache."
    ],
    "related_conditions": [
      "Iron deficiency anemia",
      "Hemochromatosis",
      "Systemic lupus erythematosus (SLE)",
      "Raynaud's disease",
      "Lichen planus",
      "Psoriasis"
    ],
    "medical_treatments": [
      "Treating the underlying cause.",
      "Oral iron supplementation for iron deficiency anemia.",
      "Phlebotomy for hemochromatosis."
    ],
    "when_to_see_doctor": [
      "If nails become spoon-shaped, to investigate for an underlying nutritional deficiency or systemic disease."
    ],
    "prognosis": "Nails typically return to a normal shape after the underlying condition (e.g., iron deficiency) is successfully treated, though it takes months for the new nail to grow out.",
    "references": [
      "https://medlineplus.gov/ency/article/003247.htm",
      "https://dermnetnz.org/topics/koilonychia",
      "https://my.clevelandclinic.org/health/diseases/23204-koilonychia-spoon-nails"
    ]
  }
